{
  "version": 2,
  "name": "moedim",
  "buildCommand": "cd apps/mobile && npm run build:web",
  "outputDirectory": "apps/mobile/dist",
  "installCommand": "npm install",
  "env": {
    "EXPO_PUBLIC_API_URL": "@railway_api_url"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}