{
  "version": 2,
  "framework": null,
  "buildCommand": "cd apps/mobile && npm run build:web",
  "outputDirectory": "apps/mobile/dist",
  "installCommand": "npm install",
  "env": {
    "EXPO_PUBLIC_API_URL": "https://moedim-api-production.up.railway.app"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; script-src 'self' 'unsafe-eval' 'unsafe-inline'; style-src 'self' 'unsafe-inline'; img-src 'self' data: https:; connect-src 'self' https://moedim-api-production.up.railway.app; font-src 'self' data:;"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}